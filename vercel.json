{
  "version": 2,

  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "functions": {
    "services/render.js": {
      "maxDuration": 10
    },
    "routes/*": {
      "maxDuration": 10
    }
  }
}
